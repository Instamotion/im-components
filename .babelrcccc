{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-typescript",
    "@babel/react"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-syntax-dynamic-import",
    "babel-plugin-styled-components",
    "react-hot-loader/babel",
    ["inline-json-import", {}]
  ],
  "env": {
    "development": {
      "compact": false
    }
  }
}
